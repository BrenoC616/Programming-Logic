Algoritmo "semnome"


Var
   i, numero, resultado, ini, fim:inteiro


Inicio

   i <- 10

   escreva("Digite um numero: ")
   leia(numero)

   enquanto i > 0 faca

      resultado <- numero - i

      escreval(numero, " -", i, " =", resultado)
      
      i <- i - 1
      
   fimenquanto

Fimalgoritmo